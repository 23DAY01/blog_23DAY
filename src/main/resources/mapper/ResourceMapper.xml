<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.day.blog.mapper.ResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="site.day.blog.pojo.domain.Resource">
        <id column="id" property="id" />
        <result column="resource_name" property="resourceName" />
        <result column="url" property="url" />
        <result column="request_method" property="requestMethod" />
        <result column="parent_id" property="parentId" />
        <result column="is_anonymous" property="isAnonymous" />
        <result column="deleted" property="deleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, resource_name, url, request_method, parent_id, is_anonymous, deleted, create_time, update_time
    </sql>

</mapper>
